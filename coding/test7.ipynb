   "source": [
    "def lstr(strs):\n",
    "\n",
    "    prefix = strs[0]\n",
    "\n",
    "    for i in strs[1:]:\n",
    "        while not i.startswith(prefix):\n",
    "            prefix = prefix[:-1]\n",
    "        \n",
    "        if not prefix:\n",
    "            return \"\"\n",
    "        \n",
    "    return prefix\n"
   ]
  }
